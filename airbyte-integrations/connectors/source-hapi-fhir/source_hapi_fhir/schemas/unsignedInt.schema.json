{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "id": "http://hl7.org/fhir/json-schema/unsignedInt",
  "$ref": "#/definitions/unsignedInt",
  "definitions" : {
    "unsignedInt": {
      "pattern": "^[0]|([1-9][0-9]*)$",
      "type": "number",
      "description": "An integer with a value that is not negative (e.g. \u003e\u003d 0)"
    }
  }
}